{
  "name": "nova",
  "version": "1.0.0",
  "description": "Thesis project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon back-end/server.js",
    "postinstall": "bower install",
    "buildDb": "npm run buildMainDb && npm run buildTransactionDb",
    "buidMainDb" : "mysql -u root -p < back-end/db/dbSchema/mainSchema.sql",
    "buildTransactionDb": "mysql -u root -p < back-end/db/dbSchema/transactionQueue.sql",
    "updateDb": "mysql -u root -p < back-end/db/dbSchema/updateSchema.sql",
    "populateDbs": "npm run populateUserTable && npm run populateTransactionHist && npm run populateScoresHist",
    "populateUserTable" : "node back-end/db/dbControllers/populateUserTable.js",
    "populateTransactionHist" : "node back-end/db/dbControllers/populateTransactionHist.js",
    "populateScoresHist" : "node back-end/db/dbControllers/populateScoresHist.js",
    "populateCurrentStocks" : "node back-end/db/dbControllers/populateCurrentStocks.js"
  },
